{
  "name": "media-app",
  "version": "0.1.0",
  "main": "dist/index.js",
  "author": "Lam Hieu <lamhieu.vk@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean:build": "rimraf dist src/dist",
    "clean:db": "rimraf data",
    "clean": "npm run clean:db && npm run clean:build",
    "kill-port:app": "kill-port --port 4000 4001",
    "kill-port:db": "kill-port --port 4010 4011",
    "kill-port": "npm run kill-port:db && npm run kill-port:app",
    "dev:app": "mode=development nodemon src/app.js",
    "dev:db": "mode=development babel-node src/mongo.js",
    "dev": "npm run kill-port && concurrently --kill-others 'npm run dev:db' 'sleep 10 && npm run dev:app'",
    "build:next": "cross-env mode=production next build dist",
    "build:app": "cross-env mode=production babel src -d dist",
    "build": "npm run build:app && npm run build:next",
    "start:app": "cross-env mode=production node dist/app.js",
    "start:db": "cross-env mode=production node dist/mongo.js",
    "serve": "npm run clean:build && npm run build && npm run start",
    "serve:app": "npm run clean:build && npm run build && npm run start:app",
    "start": "npm run kill-port && concurrently --kill-others 'npm run start:db' 'npm run start:app'",
    "lint": "eslint src",
    "flow": "flow",
    "stats": "webpack-bundle-analyzer dist/dist/server/stats.json",
    "test": "jest",
    "release": "standard-version"
  },
  "keywords": [],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "8",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.80.0",
    "jest": "^23.5.0",
    "kill-port": "^1.3.2",
    "mongodb-topology-manager": "^2.1.0",
    "next-plugin-custom-babel-config": "^1.0.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "standard-version": "^4.4.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "axios": "^0.18.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "glob": "^7.1.3",
    "lodash": "^4.17.11",
    "mongoose": "^5.2.13",
    "next": "^7.0.0-canary.14",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "styled-components": "^4.1.3",
    "url": "^0.11.0"
  }
}
